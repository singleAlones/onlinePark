/* pages/find/find.wxss */
page{
  background: url(" https://www.centurywealth.cn:443/images/find/bg.jpg");
  background-size: 100% 100%;
  display: flex;
  justify-content: center;
  align-items:flex-end;
  height: 100%;
  }
  .bg{
    background: url("https://www.centurywealth.cn:443/images/find/reg.png");
     background-size: 100% 100%;
    height:60%;
    width: 85%;
    padding-top: 60rpx;
    color:#B39FFF;
    font-family: "Microsoft YaHei";
    font-size: 36rpx;
  }
   .bg .into{
     display: flex;
    justify-content: center;
    margin-bottom: 20rpx;
     
   }
   .into input{
     border:1px solid #B39FFF;
     width:80%;
     height:80rpx;
     border-radius: 50rpx;
     font-size:28rpx;
     margin-top: 20rpx;
     padding-left: 50rpx;
     box-sizing: border-box;
     color:rgb(153, 150, 150);
   }
   .into image{
      width:80%;
     height:95rpx;
   }
   .into img{
     padding-top: 30rpx;
   }
    
  
